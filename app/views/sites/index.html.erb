<h1>Listing sites</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Subtitle</th>
      <th>Domain</th>
      <th>Subdomain</th>
      <th>Level</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Phone</th>
      <th>Headercarousel</th>
      <th>Welcomemessage</th>
      <th>Services</th>
      <th>Reviews</th>
      <th>Reservations</th>
      <th>Styles</th>
      <th>Backgroundcolor</th>
      <th>Textcolor</th>
      <th>Hightlightcolor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sites.each do |site| %>
      <tr>
        <td><%= site.title %></td>
        <td><%= site.subtitle %></td>
        <td><%= site.domain %></td>
        <td><%= site.subdomain %></td>
        <td><%= site.level %></td>
        <td><%= site.address %></td>
        <td><%= site.city %></td>
        <td><%= site.state %></td>
        <td><%= site.zip %></td>
        <td><%= site.phone %></td>
        <td><%= site.headercarousel %></td>
        <td><%= site.welcomemessage %></td>
        <td><%= site.services %></td>
        <td><%= site.reviews %></td>
        <td><%= site.reservations %></td>
        <td><%= site.styles %></td>
        <td><%= site.backgroundcolor %></td>
        <td><%= site.textcolor %></td>
        <td><%= site.hightlightcolor %></td>
        <td><%= link_to 'Show', site %></td>
        <td><%= link_to 'Edit', edit_site_path(site) %></td>
        <td><%= link_to 'Destroy', site, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Site', new_site_path %>
